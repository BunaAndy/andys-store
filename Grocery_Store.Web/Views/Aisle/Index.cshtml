@model AislesViewModel
@{
    ViewData["Title"] = "Aisle";
}

<h1 class="display-4">Aisles</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Aisles[0].AisleId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Aisles[0].Tag)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Aisles)
        {
            <tr>
                <th>
                    <a asp-action="@item.AisleId" >@Html.DisplayFor(modelItem => item.AisleId)</a>
                </th>
                <th>
                    @Html.DisplayFor(modelItem => item.Tag)
                </th>
            </tr>
        }
    </tbody>
</table>